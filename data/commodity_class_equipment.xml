<commodity-class>
	<identifier>equipment</identifier>
	<name>Equipment</name>
	<space-requirement>1000</space-requirement>
	<visualization>
		<model-identifier>cargo_cube</model-identifier>
		<visualization-parts>
			<visualization-part>
				<part-identifier>cargo_cube</part-identifier>
				<style>
					<diffuse-color>
						<red>0.95</red>
						<green>0.68</green>
						<blue>0.2</blue>
						<opacity>1.0</opacity>
					</diffuse-color>
					<specular-color>
						<valid>true</valid>
						<value>
							<red>1.0</red>
							<green>0.9</green>
							<blue>0.8</blue>
							<opacity>1.0</opacity>
						</value>
					</specular-color>
					<shininess>
						<valid>true</valid>
						<value>30.0</value>
					</shininess>
					<program-identifier>default</program-identifier>
				</style>
			</visualization-part>
		</visualization-parts>
	</visualization>
</commodity-class>
