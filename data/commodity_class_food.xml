<commodity-class>
	<identifier>food</identifier>
	<name>Food</name>
	<description>Food is required on all  planets that cannot support their colonies through their own agricultural sector.</description>
	<base-price>70</base-price>
	<space-requirement>1000</space-requirement>
	<visualization>
		<model-identifier>cargo_cube</model-identifier>
		<visualization-parts>
			<visualization-part>
				<part-identifier>cargo_cube</part-identifier>
				<style>
					<diffuse-color>
						<red>0.3</red>
						<green>0.72</green>
						<blue>0.25</blue>
						<opacity>1.0</opacity>
					</diffuse-color>
					<specular-color>
						<valid>false</valid>
						<value>
							<red>0.0</red>
							<green>0.0</green>
							<blue>0.0</blue>
							<opacity>1.0</opacity>
						</value>
					</specular-color>
					<shininess>
						<valid>true</valid>
						<value>40.0</value>
					</shininess>
					<program-identifier>default</program-identifier>
				</style>
			</visualization-part>
		</visualization-parts>
	</visualization>
</commodity-class>
