<definitions>
	<color4f>
		<float identifier="red"/>
		<float identifier="green"/>
		<float identifier="blue"/>
		<float identifier="alpha"/>
	</color4f>
	
	<vector2f>
		<float identifier="x"/>
		<float identifier="y"/>
	</vector2f>
	
	<widget>
		<string identifier="widget-path"/>
		<string identifier="name"/>
		<vector2f identifier="position"/>
		<boolean identifier="use-size"/>
		<vector2f identifier="size"/>
		<boolean identifier="use-background-color"/>
		<color4f identifier="background-color"/>
		<boolean identifier="visible"/>
	</widget>
	
	<label>
		<widget identifier="widget"/>
		<boolean identifier="use-foreground-color"/>
		<color4f identifier="foreground-color"/>
		<u1byte identifier="horizontal-alignment"/>
		<u1byte identifier="vertical-alignment"/>
	</label>
	
	<point>
		<string identifier="identifier"/>
		<string identifier="name"/>
		<vector3f identifier="position"/>
	</point>
	
	<triangle-point>
		<string identifier="identifier"/>
		<vector3f identifier="normal"/>
		<string identifier="point-identifier"/>
	</triangle-point>
	
	<triangle>
		<string identifier="identifier"/>
		<string identifier="name"/>
		<string identifier="triangle-point-identifier"/>
		<string identifier="triangle-point-identifier"/>
		<string identifier="triangle-point-identifier"/>
	</triangle>
	
	<model>
		<string identifier="identifier"/>
		<array type="point" identifier="points"/>
		<array type="triangle-point" identifier="triangle-points"/>
		<array type="triangle" identifier="triangles"/>
	</model>
</definitions>
