<commodity-class>
	<identifier>fuel</identifier>
	<name>Fuel</name>
	<space-requirement>1000</space-requirement>
	<visualization>
		<model-identifier>cargo_cube</model-identifier>
		<visualization-parts>
			<visualization-part>
				<part-identifier>cargo_cube</part-identifier>
				<material>
					<diffuse-color>
						<red>0.25</red>
						<green>0.13</green>
						<blue>0.6</blue>
						<opacity>1.0</opacity>
					</diffuse-color>
					<specular-color>
						<valid>true</valid>
						<red>0.4</red>
						<green>0.8</green>
						<blue>1.0</blue>
						<opacity>1.0</opacity>
					</specular-color>
					<shininess>80</shininess>
					<program-identifier>default</program-identifier>
				</material>
			</visualization-part>
		</visualization-parts>
	</visualization>
</commodity-class>
