<commodity-class>
	<identifier>fuel</identifier>
	<name>Fuel</name>
	<description>Fuel is used everywhere, especially on industrial planets and large trade hubs.</description>
	<base-price>200</base-price>
	<space-requirement>1000</space-requirement>
	<visualization>
		<model-identifier>cargo_cube</model-identifier>
		<visualization-parts>
			<visualization-part>
				<part-identifier>cargo_cube</part-identifier>
				<style>
					<diffuse-color>
						<red>0.25</red>
						<green>0.13</green>
						<blue>0.6</blue>
						<opacity>1.0</opacity>
					</diffuse-color>
					<specular-color>
						<valid>true</valid>
						<value>
							<red>0.4</red>
							<green>0.8</green>
							<blue>1.0</blue>
							<opacity>1.0</opacity>
						</value>
					</specular-color>
					<shininess>
						<valid>true</valid>
						<value>80.0</value>
					</shininess>
					<program-identifier>default</program-identifier>
				</style>
			</visualization-part>
		</visualization-parts>
	</visualization>
</commodity-class>
