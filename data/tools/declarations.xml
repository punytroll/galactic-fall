<declarations>
	
	<accepted-slot-class is="string"/>
	
	<asset-class>
		<string identifier="identifier"/>
		<string identifier="name"/>
		<float identifier="base-price"/>
		<string identifier="object-type"/>
		<string identifier="object-class"/>
	</asset-class>
	
	<color4f>
		<float identifier="red"/>
		<float identifier="green"/>
		<float identifier="blue"/>
		<float identifier="alpha"/>
	</color4f>
	
	<commodity-class>
		<string identifier="identifier"/>
		<string identifier="name"/>
		<float identifier="space-requirement"/>
		<visualization identifier="visualization"/>
	</commodity-class>
	
	<label>
		<widget identifier="widget"/>
		<boolean identifier="use-text-color"/>
		<color4f identifier="text-color"/>
		<u1byte identifier="horizontal-alignment"/>
		<u1byte identifier="vertical-alignment"/>
	</label>
	
	<material>
		<boolean identifier="use-specular-color"/>
		<color4f identifier="diffuse-color"/>
		<color4f identifier="specular-color"/>
