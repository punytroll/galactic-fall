		<float identifier="reload-time"/>
		<float identifier="space-requirement"/>
		<float identifier="energy-usage-per-shot"/>
		<particle identifier="particle"/>
	</weapon-class>
	
	<widget>
		<string identifier="widget-path"/>
		<string identifier="name"/>
		<vector2f identifier="position"/>
		<boolean identifier="use-size"/>
		<vector2f identifier="size"/>
		<boolean identifier="use-background-color"/>
		<color4f identifier="background-color"/>
		<boolean identifier="visible"/>
		<boolean identifier="anchor-bottom"/>
		<boolean identifier="anchor-left"/>
		<boolean identifier="anchor-right"/>
		<boolean identifier="anchor-top"/>
	</widget>
	
