<declarations>
	
	<accepted-slot-class is="string"/>
	
	<asset-class type="4">
		<string identifier="identifier"/>
		<string identifier="name"/>
		<float identifier="base-price"/>
		<string identifier="description"/>
		<string identifier="object-type"/>
		<string identifier="object-class"/>
	</asset-class>
	
	<battery-class type="12">
		<string identifier="identifier"/>
		<string identifier="name"/>
		<visualization identifier="visualization"/>
		<u4byte identifier="space-requirement"/>
		<float identifier="energy-capacity"/>
		<string identifier="slot-class-identifier"/>
	</battery-class>
	
	<color type="18"/>
	
	<color4f type="18" sub-type="0">
		<float identifier="red"/>
		<float identifier="green"/>
		<float identifier="blue"/>
		<float identifier="opacity"/>
	</color4f>
	
	<commodity-class type="8">
		<string identifier="identifier"/>
		<string identifier="name"/>
		<u4byte identifier="space-requirement"/>
		<visualization identifier="visualization"/>
	</commodity-class>
	
	<dimensions type="15"/>
	
	<dimensions2f type="15" sub-type="0">
		<float identifier="width"/>
		<float identifier="height"/>
	</dimensions2f>
	
	<faction type="14">
		<string identifier="identifier"/>
		<string identifier="name"/>
		<color4f identifier="color"/>
	</faction>
	
	<float type="21" sub-type="0"/>
	
	<fragment-shader is="shader" type="19" sub-type="1"/>
	
	<generator-class type="13">
		<string identifier="identifier"/>
		<string identifier="name"/>
		<u4byte identifier="space-requirement"/>
		<float identifier="energy-provision-per-second"/>
		<string identifier="slot-class-identifier"/>
	</generator-class>
	
	<key-binding>
		<u4byte identifier="code"/>
		<string identifier="event"/>
		<string identifier="action"/>
	</key-binding>
	
	<key-binding-profile type="16" is="collection" element-type="key-binding"/>
	
	<style>
		<color4f identifier="diffuse-color"/>
		<optional-color4f identifier="specular-color"/>
		<optional-float identifier="shininess"/>
		<string identifier="program-identifier"/>
	</style>
	
	<mesh type="2">
		<string identifier="identifier"/>
		<collection identifier="points" element-type="point"/>
		<collection identifier="triangle-points" element-type="triangle-point"/>
		<collection identifier="triangles" element-type="triangle"/>
	</mesh>
	
	<model type="11">
		<string identifier="identifier"/>
		<collection identifier="model-parts" element-type="model-part"/>
	</model>
	
	<model-part>
		<string identifier="identifier"/>
		<string identifier="mesh-identifier"/>
		<vector3f identifier="position"/>
		<quaternion identifier="orientation"/>
	</model-part>
	
	<name-aspect>
		<string identifier="name"/>
	</name-aspect>
	
	<offered-asset-class>
		<string identifier="identifier"/>
		<float identifier="base-price-modifier"/>
	</offered-asset-class>
	
	<optional-color4f type="18" sub-type="1">
		<boolean identifier="valid"/>
		<float identifier="red"/>
		<float identifier="green"/>
		<float identifier="blue"/>
		<float identifier="opacity"/>
	</optional-color4f>
	
	<optional-float type="21" sub-type="1">
		<boolean identifier="valid"/>
		<float identifier="value"/>
	</optional-float>
	
	<particle>
		<vector3f identifier="exit-position"/>
		<float identifier="exit-speed"/>
		<float identifier="damage"/>
		<float identifier="life-time"/>
		<visualization identifier="visualization"/>
	</particle>
	
	<physical-aspect>
		<u4byte identifier="space-requirement"/>
	</physical-aspect>
	
	<planet>
		<string identifier="identifier"/>
		<string identifier="name"/>
		<string identifier="description"/>
		<visualization identifier="visualization"/>
		<vector2f identifier="position"/>
		<float identifier="size"/>
		<collection identifier="offered-asset-classes" element-type="offered-asset-class"/>
		<float identifier="landing-fee-per-space"/>
		<boolean identifier="offers-recharging"/>
		<float identifier="recharging-fee-per-energy"/>
		<boolean identifier="offers-repairing"/>
		<float identifier="repairing-fee-per-hull"/>
		<string identifier="faction-identifier"/>
	</planet>
	
	<point>
		<string identifier="identifier"/>
		<string identifier="name"/>
		<vector3f identifier="position"/>
	</point>
	
	<shader type="19">
		<string identifier="identifier"/>
		<string identifier="source"/>
	</shader>
	
	<shader-identifier is="string"/>
	
	<shader-program type="20" sub-type="0">
		<string identifier="identifier"/>
		<collection identifier="shaders" element-type="shader-identifier"/>
	</shader-program>
	
	<quaternion>
		<float identifier="w"/>
		<float identifier="x"/>
		<float identifier="y"/>
		<float identifier="z"/>
	</quaternion>
	
	<scenario type="17">
		<string identifier="identifier"/>
		<string identifier="name"/>
		<string identifier="description"/>
		<string identifier="savegame"/>
	</scenario>
	
	<ship-class type="3">
		<string identifier="identifier"/>
		<name-aspect identifier="name-aspect"/>
		<physical-aspect identifier="physical-aspect"/>
		<visualization identifier="visualization"/>
		<float identifier="forward-thrust"/>
		<float identifier="turn-speed"/>
		<float identifier="maximum-speed"/>
		<float identifier="maximum-available-space"/>
		<float identifier="fuel-hold-size"/>
		<float identifier="jump-fuel"/>
		<float identifier="forward-fuel"/>
		<float identifier="turn-fuel"/>
		<float identifier="hull"/>
		<vector3f identifier="exhaust-offset"/>
		<float identifier="exhaust-radius"/>
		<collection identifier="slots" element-type="slot"/>
	</ship-class>
	
	<slot>
		<string identifier="identifier"/>
		<string identifier="class-identifier"/>
		<string identifier="name"/>
		<vector3f identifier="position"/>
		<quaternion identifier="orientation"/>
		<boolean identifier="visualize-accessory"/>
	</slot>
	
	<slot-class type="9">
		<string identifier="identifier"/>
		<string identifier="name"/>
		<collection identifier="accepted-slot-classes" element-type="accepted-slot-class"/>
	</slot-class>
	
	<star>
		<string identifier="identifier"/>
		<vector2f identifier="position"/>
		<color4f identifier="color"/>
	</star>
	
	<system type="5">
		<string identifier="identifier"/>
		<string identifier="name"/>
		<vector2f identifier="position"/>
		<float identifier="traffic-density"/>
		<star identifier="star"/>
		<collection identifier="planets" element-type="planet"/>
	</system>
	
	<system-link type="6">
		<string identifier="system-1-identifier"/>
		<string identifier="system-2-identifier"/>
	</system-link>
	
	<texture type="10" sub-type="0">
		<string identifier="identifier"/>
		<bytes identifier="data"/>
	</texture>
	
	<turret-class type="22" sub-type="0">
		<string identifier="identifier"/>
		<string identifier="name"/>
		<visualization identifier="visualization"/>
		<string identifier="slot-class-identifier"/>
		<quaternion identifier="orientation"/>
		<float identifier="reload-time"/>
		<u4byte identifier="space-requirement"/>
		<float identifier="energy-usage-per-shot"/>
		<particle identifier="particle"/>
	</turret-class>
	
	<triangle>
		<string identifier="identifier"/>
		<string identifier="name"/>
		<string identifier="triangle-point-1-identifier"/>
		<string identifier="triangle-point-2-identifier"/>
		<string identifier="triangle-point-3-identifier"/>
	</triangle>
	
	<triangle-point>
		<string identifier="identifier"/>
		<vector3f identifier="normal"/>
		<string identifier="point-identifier"/>
	</triangle-point>
	
	<vector type="1"/>
	
	<vector2f type="1" sub-type="0">
		<float identifier="x"/>
		<float identifier="y"/>
	</vector2f>
	
	<vector3f type="1" sub-type="1">
		<float identifier="x"/>
		<float identifier="y"/>
		<float identifier="z"/>
	</vector3f>
	
	<vertex-shader is="shader" type="19" sub-type="0"/>
	
	<visualization>
		<string identifier="model-identifier"/>
		<collection identifier="visualization-parts" element-type="visualization-part"/>
	</visualization>
	
	<visualization-part>
		<string identifier="part-identifier"/>
		<style identifier="style"/>
	</visualization-part>
	
	<weapon-class type="7">
		<string identifier="identifier"/>
		<string identifier="name"/>
		<visualization identifier="visualization"/>
		<string identifier="slot-class-identifier"/>
		<quaternion identifier="orientation"/>
		<float identifier="reload-time"/>
		<u4byte identifier="space-requirement"/>
		<float identifier="energy-usage-per-shot"/>
		<particle identifier="particle"/>
	</weapon-class>
	
</declarations>
