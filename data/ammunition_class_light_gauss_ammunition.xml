<ammunition-class>
	<identifier>light_gauss_ammunition</identifier>
	<name>Light Gauss Ammunition</name>
	<description>This is a cartridge of 25 shells for light gauss cannons. The shell diameter is 4 cm, the length of the shell is 20 cm.</description>
	<base-price>50</base-price>
	<space-requirement>8</space-requirement>
	<visualization>
		<model-identifier>light_gauss_ammunition</model-identifier>
		<visualization-parts>
			<visualization-part>
				<part-identifier>light_gauss_ammunition</part-identifier>
				<style>
					<diffuse-color>
						<red>0.2</red>
						<green>0.2</green>
						<blue>0.4</blue>
						<opacity>1.0</opacity>
					</diffuse-color>
					<specular-color>
						<valid>true</valid>
						<value>
							<red>0.9</red>
							<green>0.8</green>
							<blue>0.8</blue>
							<opacity>1.0</opacity>
						</value>
					</specular-color>
					<shininess>
						<valid>true</valid>
						<value>80.0</value>
					</shininess>
					<program-identifier>default</program-identifier>
				</style>
			</visualization-part>
		</visualization-parts>
	</visualization>
	<cartridge-size>25</cartridge-size>
</ammunition-class>
